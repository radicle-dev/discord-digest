{
  "name": "discord-digest",
  "version": "0.0.1",
  "main": "functions/index.js",
  "config": {
    "function_name": "discordDigest",
    "region": "europe-west1",
    "gcp_project": "discord-digest",
    "runtime": "nodejs16"
  },
  "scripts": {
    "dev": "nodemon --exec 'npm run start' -e ts,js --ignore functions/",
    "start": "npm run clean && tsc && npx functions-framework --target=discordDigest --signature-type=http",
    "clean": "rimraf \"functions/*\"",
    "lint": "tslint --project '.'",
    "tsc": "tsc",
    "cp-package": "copyfiles package.json functions",
    "build": "npm run clean && npm run lint && tsc && npm run cp-package",
    "deploy": "npm run build && npm run deploy-gcp",
    "deploy-gcp": "gcloud functions deploy $npm_package_config_function_name --runtime $npm_package_config_runtime --region=$npm_package_config_region --trigger-http --project $npm_package_config_gcp_project"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "@types/express": "^4.17.13",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
